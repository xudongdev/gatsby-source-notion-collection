{
  "name": "gatsby-source-notion-collection",
  "version": "0.0.0-development",
  "description": "",
  "keywords": [
    "gatsby",
    "notion"
  ],
  "author": {
    "name": "Xudong Huang",
    "email": "me@xudong.dev",
    "url": "https://www.xudong.dev/"
  },
  "bugs": "https://github.com/xudongdev/gatsby-source-notion-collection/issues",
  "homepage": "https://github.com/xudongdev/gatsby-source-notion-collection#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/xudongdev/gatsby-source-notion-collection.git"
  },
  "license": "MIT",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "lint": "eslint **/**.js",
    "semantic-release": "semantic-release"
  },
  "dependencies": {
    "@mapbox/rehype-prism": "^0.5.0",
    "axios": "^0.20.0",
    "bluebird": "^3.7.2",
    "lodash": "^4.17.19",
    "rehype-parse": "^7.0.1",
    "rehype-stringify": "^8.0.0",
    "remark-html": "^12.0.0",
    "remark-rehype": "^8.0.0",
    "remark-stringify": "^8.1.1",
    "unified": "^9.1.0"
  },
  "devDependencies": {
    "commitizen": "^4.1.2",
    "cz-conventional-changelog": "^3.2.0",
    "eslint": "^7.5.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "^23.19.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
    "prettier": "^2.0.5",
    "semantic-release": "^17.1.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true"
    }
  },
  "lint-staged": {
    "*.{json,js,md}": [
      "prettier --write"
    ],
    "*.{js}": [
      "eslint --fix"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "release": {
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/github",
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/npm",
        {
          "npmPublish": true
        }
      ]
    ]
  }
}
